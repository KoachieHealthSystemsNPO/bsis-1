#locationsTable {
    margin-top: 30px;
    width: 100%;
    border: 3px outset #BAB6B8;
    border-collapse: collapse;
}

#locationsTable th, td {
    border: 1px solid #BAB6B8;
    padding: 1px;
    font-weight: normal;
    text-align: center;
    width: 10%;
}

#locationsTable th {
    background-color: #C9C5C7;
}