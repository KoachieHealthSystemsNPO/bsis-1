#collectionReportTable {
    border: 3px outset #BAB6B8;
    border-collapse: collapse;
    margin: 20px 0 0 1px;
    width: 871px;
}

#collectionReportTable th, td {
    border: 1px solid #BAB6B8;
    padding: 1px;
    font-weight: normal;
